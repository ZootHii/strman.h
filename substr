char *substr(char dest[], char src[], int start, int end, int dest_len){

	int sub_len;
	int len = strlen(src);
 

	if (end > len - 1)
		end = len - 1; 
 
	if (start < 0) 
		start = 0; 
 
 
	if (start > end)
		dest[0] = '\0';
	
	else{
		
		sub_len = end - start + 1; 
		
		if (sub_len >= dest_len) 
			sub_len = dest_len - 1; 
 
		memmove(dest, src + start, sub_len); 
		dest[sub_len] = '\0'; 
	} 
 
	return(dest); 
 
}
